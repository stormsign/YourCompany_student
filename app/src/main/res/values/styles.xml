<resources>

  <style name="AppTheme" parent="AppTheme.Base">
    <!-- Customize your theme here. -->
  </style>

  <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="colorPrimary">@color/themeColor</item>
    <item name="colorPrimaryDark">@color/themeColor</item>
    <item name="android:windowBackground">@android:color/background_light</item>
    <item name="toolbarStyle">@style/customtoolbar</item>
    <item name="colorAccent">@color/themeColor</item>

  </style>
  <style name="MyAlertDialogStyle" parent="@style/Theme.AppCompat.Light.Dialog.Alert">
    <item name="android:textColor">@color/textDarkone</item>
    <item name="colorAccent">@color/textDarkone</item>
    <item name="android:background">@android:color/white</item>
  </style>
  <style name="AppTheme.Base_TWO" parent="@style/Theme.AppCompat.Light">
    <item name="windowActionBar">false</item>
    <item name="windowNoTitle">true</item>
  </style>

  <style name="CollapsingToolbarColorPrimary" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimary">@android:color/white</item>
    <item name="colorAccent">@android:color/white</item>

  </style>

  <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
    <item name="android:textSize">18sp</item>

  </style>

  <style name="customtoolbar" parent="ThemeOverlay.AppCompat.Light"></style>

  <style name="ThemeGalleryActivity" parent="Theme.AppCompat.Light.DarkActionBar">
    <item name="windowNoTitle">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowNoTitle">true</item>
    <item name="windowActionBar">false</item>
    <item name="android:windowAnimationStyle">@null</item>
  </style>

  <style name="progressBarStyleHorizontal"
      parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
    <item name="android:progressDrawable">@drawable/progress_horizontal</item>
    <item name="android:minHeight">16dip</item>
    <item name="android:maxHeight">16dip</item>
  </style>

  <style name="DialogButton">
    <item name="android:textAllCaps">true</item>
    <item name="android:clickable">true</item>
    <item name="android:background">?android:selectableItemBackground</item>
    <item name="android:paddingTop">12dp</item>
    <item name="android:paddingBottom">12dp</item>
    <item name="android:paddingLeft">@dimen/ld_side_margin</item>
    <item name="android:paddingRight">@dimen/ld_side_margin</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textColor">?android:textColorPrimary</item>
  </style>
  <style name="dialog_animation" parent="android:Animation">
    <item name="@android:windowEnterAnimation">@anim/dialog_enter</item>
    <item name="@android:windowExitAnimation">@anim/dialog_exit</item>
  </style>
  <style name="dialog_common" parent="android:style/Theme.Dialog">
    <item name="android:windowBackground">@color/white</item>
  </style>
  <style name="dialog_bottom" parent="@style/dialog_common">
    <item name="android:windowBackground">@color/white</item>
    <item name="android:windowAnimationStyle">@style/dialog_animation</item>
    <item name="android:textColor">@color/text_black</item>
    <item name="android:typeface">monospace</item>
    <item name="android:textSize">14sp</item>
  </style>
  <!-- 分享对话框样式 start -->
  <style name="dialog_content_share_ll">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:gravity">center_horizontal</item>
    <item name="android:orientation">vertical</item>
  </style>

  <style name="dialog_content_share_text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:drawablePadding">4dp</item>
    <item name="android:textColor">@color/text_black</item>
    <item name="android:gravity">center</item>
    <item name="android:textSize">12sp</item>
  </style>

  <style name="CheckBoxTintTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
    <item name="colorAccent">@color/themeColor</item>
  </style>

  <style name="TintTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
    <item name="colorAccent">@color/themeColor</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowBackground">@android:color/transparent</item>
  </style>

  <style name="WelcomeHeadingAppearance" parent="@android:style/TextAppearance.Large">
    <item name="android:textSize">@dimen/welcome_heading</item>
    <item name="android:textColor">@android:color/white</item>
  </style>

  <style name="WelcomeContentAppearance" parent="@android:style/TextAppearance">
    <item name="android:textSize">@dimen/welcome_content</item>
    <item name="android:textColor">@android:color/white</item>
  </style>

  <style name="user_info_imageview_two">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:layout_alignParentRight">true</item>
    <item name="android:src">@mipmap/list_ico_jiantou</item>
  </style>

  <style name="user_account_text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textSize">@dimen/text_15</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:textColor">@color/textDarktwo</item>
    <item name="android:layout_marginLeft">16dp</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="user_info_imageview">
    <item name="android:layout_width">15dp</item>
    <item name="android:layout_height">15dp</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:layout_gravity">center_vertical</item>
    <item name="android:layout_alignParentRight">true</item>
    <item name="android:src">@mipmap/list_ico_jiantou</item>
    <item name="android:layout_marginRight">10dp</item>
  </style>

  <style name="me_ico_style">
    <item name="android:layout_width">25dp</item>
    <item name="android:layout_height">25dp</item>
    <item name="android:layout_alignParentLeft">true</item>
    <item name="android:layout_centerVertical">true</item>
  </style>

  <style name="user_info_text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textSize">@dimen/text_16</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:textColor">@color/textDarkone</item>
    <item name="android:layout_marginLeft">10dp</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="user_info_text_v">
    <item name="android:maxLength">12</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textSize">@dimen/text_15</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:textColor">@color/textDarkthree</item>
    <item name="android:layout_marginRight">10dp</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="text_style">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textSize">@dimen/text_15</item>
    <item name="android:textColor">@color/textDarkone</item>
  </style>

  <style name="BrandedSwitch.control" parent="Theme.AppCompat.Light">
    <!-- active thumb & track color (30% transparency) -->
    <item name="colorControlActivated">@color/themeColor</item>
  </style>

  <style name="text_dark_one">
    <item name="android:textColor">@color/textDarkone</item>
    <item name="android:textSize">@dimen/text_16</item>
  </style>

  <style name="LodingDialog" parent="Theme.AppCompat.Light.Dialog">
    <item name="android:windowBackground">@android:color/transparent</item>
  </style>

  <style name="CollapsingToolbarTitleStyle" parent="@android:style/TextAppearance">
    <item name="android:textColor">@android:color/transparent</item>
  </style>

  <style name="button_commit">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">45dp</item>
    <item name="android:background">@drawable/selector_orange_button_round</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:textSize">@dimen/text_16</item>

  </style>


  <style name="STextAppearance.RecurrenceOptionCreatorLabelStyle">
    <item name="android:textSize">15sp</item>
  </style>

  <style name="SublimeDatePickerStyle">
    <item name="spFirstDayOfWeek">0</item>
    <item name="spHeaderBackground">?spHeaderBackgroundPtr</item>
    <item name="spMinDate">01/01/1900</item>
    <item name="spMaxDate">12/31/2100</item>
    <item name="spHeaderTextColor">?attr/spPrimaryTextSecondaryWhenActivatedMaterialInversePtr
    </item>
  </style>

  <style name="SPDateLabelTextAppearance" parent="TextAppearance.AppCompat">
    <item name="android:textColor">?attr/spPrimaryTextSecondaryWhenActivatedMaterialInversePtr
    </item>
    <item name="android:textSize">@dimen/sp_date_picker_date_label_size</item>
    <item name="android:fontFamily">sans-serif-medium</item>
  </style>

  <style name="SPDateRangeLabelTextAppearance" parent="TextAppearance.AppCompat">
    <item name="android:textColor">?attr/spPrimaryTextSecondaryWhenActivatedMaterialInversePtr
    </item>
    <item name="android:textSize">@dimen/sp_date_picker_date_range_label_size</item>
    <item name="android:fontFamily">sans-serif-medium</item>
  </style>

  <style name="SPMonthLabelTextAppearance" parent="TextAppearance.AppCompat.Body2"/>

  <style name="SPWeekDayLabelTextAppearance" parent="TextAppearance.AppCompat.Caption"/>

  <!-- TextAppearance.Material.Widget.Calendar.Day -->
  <style name="SPDayTextAppearance" parent="TextAppearance.AppCompat.Caption">
    <item name="android:textColor">?attr/spCalendarDayTextColorPtr</item>
  </style>

  <!-- YearPickerView -->

  <style name="YearPickerViewStyle">
    <item name="spYearListItemTextAppearance">@style/STextAppearance.DatePicker.List.YearLabel
    </item>
    <item name="spYearListSelectorColor">?attr/colorControlActivated</item>
    <item name="spYearPickerBgColor">@android:color/transparent</item>
  </style>

  <style name="SPYearLabelTextAppearance" parent="TextAppearance.AppCompat">
    <item name="android:textColor">?android:attr/textColorPrimary</item>
    <item name="android:textSize">@dimen/sp_datepicker_list_year_label_size</item>
    <item name="android:fontFamily">sans-serif</item>
  </style>

  <style name="SPYearLabelActivatedTextAppearance" parent="TextAppearance.AppCompat">
    <item name="android:textColor">?attr/colorControlActivated</item>
    <item name="android:textSize">@dimen/sp_datepicker_list_year_activated_label_size</item>
    <item name="android:fontFamily">sans-serif-medium</item>
  </style>

  <style name="STextAppearance.DatePicker.List.YearLabel" parent="STextAppearance">
    <item name="android:textColor">?android:attr/textColorSecondary</item>
    <item name="android:textSize">@dimen/datepicker_year_label_text_size</item>
  </style>

  <!-- DayPickerView -->

  <style name="DayPickerViewStyle">
    <item name="spDayPickerBgColor">@android:color/transparent</item>
    <item name="spMonthTextAppearance">@style/SPMonthLabelTextAppearance</item>
    <item name="spWeekDayTextAppearance">@style/SPWeekDayLabelTextAppearance</item>
    <item name="spDateTextAppearance">@style/SPDayTextAppearance</item>
    <item name="spDaySelectorColor">@color/themeColor</item>
    <item name="spDayHighlightColor">?attr/colorControlHighlight</item>
  </style>

  <!-- SimpleMonthView -->

  <style name="MonthViewStyle">
    <item name="spSelectedDayCircleAlpha">60</item>
    <item name="spCalendarEnabledTextColor">?android:attr/textColorSecondary</item>
    <item name="spCalendarDisabledTextColor">?attr/spCalendarDisabledTextColor</item>
    <item name="spCalendarEnabledSelectedTextColor">?attr/colorAccent</item>
    <item name="spMonthNameTextColor">?android:attr/textColorSecondary</item>
    <item name="spDayNameTextColor">?android:attr/textColorSecondary</item>
  </style>

  <style name="STextAppearanceBase">
    <item name="android:textColor">?android:attr/textColorPrimary</item>
    <item name="android:textColorHint">?android:attr/textColorHint</item>
    <item name="android:textColorHighlight">?android:attr/textColorHighlight</item>
    <item name="android:textColorLink">?android:attr/textColorLink</item>
    <item name="android:textSize">@dimen/text_size_body_1</item>
    <item name="android:textStyle">normal</item>
  </style>

  <style name="STextAppearance" parent="STextAppearanceBase"/>

  <style name="STextAppearance.DatePicker.DayOfWeekLabel" parent="STextAppearance">
    <item name="android:includeFontPadding">false</item>
    <item name="android:textColor">?android:attr/textColorPrimaryInverse</item>
    <item name="android:textSize">@dimen/datepicker_header_text_size</item>
  </style>

  <style name="STextAppearance.DatePicker.MonthLabel" parent="STextAppearance">
    <item name="android:includeFontPadding">false</item>
    <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
    <item name="android:textSize">@dimen/datepicker_selected_date_month_size</item>
  </style>

  <style name="STextAppearance.DatePicker.DayOfMonthLabel" parent="STextAppearance">
    <item name="android:includeFontPadding">false</item>
    <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
    <item name="android:textSize">@dimen/datepicker_selected_date_day_size</item>
  </style>

  <style name="STextAppearance.DatePicker.YearLabel" parent="STextAppearance">
    <item name="android:includeFontPadding">false</item>
    <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
    <item name="android:textSize">@dimen/datepicker_selected_date_year_size</item>
  </style>

  <style name="SButtonBarButtonStyleBase">
    <item name="android:textAppearance">@style/SButtonTextAppearance</item>
    <item name="android:focusable">true</item>
    <item name="android:clickable">true</item>
    <item name="android:gravity">center_vertical|center_horizontal</item>
    <item name="android:textColor">?attr/colorAccent</item>
    <item name="android:minWidth">64dp</item>
    <item name="android:maxLines">1</item>
    <item name="android:minHeight">@dimen/sp_dialog_button_bar_height</item>
  </style>

  <style name="SButtonBarButtonStyle" parent="SButtonBarButtonStyleBase"/>

  <!-- Used when extending the partition through Button bar
       See land/styles.xml -->
  <style name="SInvertedButtonBarButtonStyle" parent="SButtonBarButtonStyleBase"/>

  <style name="STextAppearanceBase.BaseButton">
    <item name="android:textSize">@dimen/text_size_button_material</item>
    <item name="android:textAllCaps">true</item>
    <item name="android:textColor">?android:attr/textColorPrimary</item>
  </style>

  <style name="SButtonTextAppearance" parent="STextAppearanceBase.BaseButton"/>

  <style name="SButtonBarStyle">
    <item name="android:background">@null</item>
  </style>

  <!-- TimePicker -->

  <style name="SublimeTimePickerStyle">
    <item name="spHeaderBackground">?spHeaderBackgroundPtr</item>
    <item name="spHeaderTextColor">?attr/spPrimaryTextSecondaryWhenActivatedMaterialInversePtr
    </item>
  </style>

  <style name="SPTimeLabelTextAppearance" parent="TextAppearance.AppCompat">
    <item name="android:textSize">@dimen/sp_timepicker_time_label_size</item>
    <item name="android:textColor">?attr/spPrimaryTextSecondaryWhenActivatedMaterialInversePtr
    </item>
  </style>

  <style name="SPAmPmLabelTextAppearance" parent="TextAppearance.AppCompat.Button">
    <item name="android:textSize">@dimen/sp_timepicker_ampm_label_size</item>
    <item name="android:textColor">?attr/spPrimaryTextSecondaryWhenActivatedMaterialInversePtr
    </item>
  </style>

  <!-- RadialTimePickerView -->

  <style name="RadialTimePickerViewStyle">
    <item name="spNumbersTextColor">?attr/spNumbersTextColorPtr</item>
    <item name="spNumbersSelectorColor">?attr/colorControlActivated</item>
    <item name="spNumbersBackgroundColor">?attr/spNumbersBackgroundColorPtr</item>
    <item name="spNumbersInnerTextColor">?attr/spNumbersInnerTextColorPtr</item>
  </style>

  <style name="STextAppearance.TimePicker.AmPmLabel" parent="STextAppearance">
    <item name="android:textSize">@dimen/timepicker_ampm_label_size</item>
    <item name="android:textAllCaps">true</item>
    <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="STextAppearance.TimePicker.TimeLabel" parent="STextAppearance">
    <item name="android:textSize">@dimen/timepicker_time_label_size</item>
    <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
  </style>

  <!-- SublimeRecurrencePicker -->

  <style name="SublimeRecurrencePickerStyle">
    <item name="spRecurrencePickerHeadingStyle">@style/RecurrencePickerHeadingStyle</item>
    <item name="spRecurrencePickerOptionsStyle">@style/RecurrencePickerOptionsStyle</item>
    <item name="spHeaderBackground">?attr/colorAccent</item>
    <item name="spPickerBackground">@android:color/transparent</item>
    <item name="spSelectedOptionTextColor">?attr/colorAccent</item>
    <item name="spUnselectedOptionsTextColor">?android:attr/textColorPrimary</item>
    <item name="spPressedOptionBgColor">?attr/colorControlHighlight</item>
    <item name="spSelectedOptionDrawable">@mipmap/checkmark_medium_ff</item>
  </style>

  <style name="RecurrencePickerHeadingStyle">
    <item name="android:textAppearance">
      @style/STextAppearance.RecurrencePickerHeadingTextAppearance
    </item>
    <item name="android:paddingLeft">24dp</item>
    <item name="android:paddingRight">24dp</item>
    <item name="android:paddingTop">16dp</item>
    <item name="android:paddingBottom">16dp</item>
  </style>

  <style name="STextAppearance.RecurrencePickerHeadingTextAppearance">
    <item name="android:textSize">15sp</item>
    <item name="android:textAllCaps">true</item>
    <item name="android:textColor">?android:attr/textColorPrimaryInverse</item>
  </style>

  <style name="RecurrencePickerOptionsStyle">
    <item name="android:textAppearance">
      @style/STextAppearance.RecurrencePickerOptionsTextAppearance
    </item>
    <item name="android:paddingLeft">24dp</item>
    <item name="android:paddingRight">24dp</item>
    <item name="android:paddingTop">16dp</item>
    <item name="android:paddingBottom">16dp</item>
    <item name="android:gravity">center_vertical</item>
  </style>

  <style name="STextAppearance.RecurrencePickerOptionsTextAppearance">
    <item name="android:textSize">14sp</item>
    <item name="android:textAllCaps">true</item>
    <item name="android:textColor">?android:attr/textColorPrimary</item>
    <item name="android:maxLines">2</item>
    <item name="android:ellipsize">end</item>
  </style>

  <!-- RecurrenceOptionCreator -->

  <style name="RecurrenceOptionCreatorStyle">
    <item name="spHeaderBackground">?attr/colorAccent</item>
    <item name="spEndDateFormat">spMediumFormat</item>
    <item name="spRocLabelTextAppearance">
      @style/STextAppearance.RecurrenceOptionCreatorLabelStyle
    </item>
    <item name="spRocWeekButtonStyle">
      @style/RecurrenceOptionCreatorWeekButtonStyle
    </item>
    <item name="spRocEndDateStyle">
      @style/RecurrenceOptionCreatorEndDateStyle
    </item>
    <item name="spRocFreqSpinnerStyle">@style/RecurrenceOptionCreatorFreqSpinnerStyle</item>
    <item name="spRocEndSpinnerStyle">@style/RecurrenceOptionCreatorEndSpinnerStyle</item>
    <item name="spRocFreqSpinnerItemStyle">@style/FreqSpinnerItemStyle</item>
    <item name="spRocEndSpinnerItemStyle">@style/EndSpinnerItemStyle</item>
    <item name="spRocEndSpinnerDropDownItemStyle">@style/EndSpinnerDropDownItemStyle</item>
    <item name="spWeekButtonUnselectedTextColor">?attr/colorAccent</item>
    <item name="spWeekButtonSelectedTextColor">?android:attr/textColorPrimaryInverse</item>
    <item name="spWeekButtonSelectedCircleColor">?attr/colorAccent</item>
  </style>


  <style name="RecurrenceOptionCreatorWeekButtonStyle">
    <item name="android:textAllCaps">true</item>
    <item name="android:textSize">12sp</item>
    <item name="android:singleLine">true</item>
    <item name="android:gravity">center</item>
  </style>

  <style name="RecurrenceOptionCreatorEndDateStyle">
    <item name="android:paddingLeft">16dp</item>
    <item name="android:paddingRight">16dp</item>
    <item name="android:textAppearance">@style/STextAppearance.EndDate</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:singleLine">true</item>
    <item name="android:minWidth">80dp</item>
    <item name="android:contentDescription">
      @string/acessibility_recurrence_choose_end_date_description
    </item>
    <item name="android:gravity">center_horizontal|bottom</item>
    <item name="android:paddingTop">12dp</item>
    <item name="android:paddingBottom">12dp</item>
    <item name="android:visibility">gone</item>
  </style>

  <style name="STextAppearance.EndDate">
    <item name="android:textSize">15sp</item>
    <item name="android:textColor">?android:attr/textColorPrimary</item>
  </style>

  <style name="EndSpinnerDropDownItemStyle" parent="Widget.AppCompat.DropDownItem.Spinner">
    <item name="android:paddingLeft">16dp</item>
    <item name="android:paddingRight">16dp</item>
    <item name="android:textAppearance">@style/STextAppearance.EndSpinnerDropDownItem</item>
    <item name="android:paddingTop">12dp</item>
    <item name="android:paddingBottom">12dp</item>
    <item name="android:singleLine">true</item>
    <item name="android:ellipsize">end</item>
  </style>

  <style name="STextAppearance.EndSpinnerDropDownItem">
    <item name="android:textSize">15sp</item>
    <item name="android:textColor">?android:attr/textColorPrimary</item>
  </style>

  <style name="EndSpinnerItemStyle" parent="Widget.AppCompat.TextView.SpinnerItem">
    <item name="android:paddingLeft">16dp</item>
    <item name="android:paddingRight">16dp</item>
    <item name="android:paddingTop">12dp</item>
    <item name="android:paddingBottom">12dp</item>
    <item name="android:textAppearance">@style/STextAppearance.EndSpinnerItem</item>
    <item name="android:singleLine">true</item>
    <item name="android:ellipsize">end</item>
  </style>

  <style name="STextAppearance.EndSpinnerItem">
    <item name="android:textSize">15sp</item>
    <item name="android:textColor">?android:attr/textColorPrimary</item>
  </style>

  <style name="FreqSpinnerItemStyle" parent="Widget.AppCompat.TextView.SpinnerItem">
    <item name="android:paddingLeft">16dp</item>
    <item name="android:paddingRight">16dp</item>
    <item name="android:paddingTop">12dp</item>
    <item name="android:paddingBottom">12dp</item>
    <item name="android:textAppearance">@style/STextAppearance.FreqSpinnerItem</item>
    <item name="android:singleLine">true</item>
    <item name="android:ellipsize">end</item>
  </style>

  <style name="STextAppearance.FreqSpinnerItem">
    <item name="android:textSize">15sp</item>
    <item name="android:textColor">?android:attr/textColorPrimaryInverse</item>
  </style>

  <style name="BaseRecurrenceOptionCreatorFreqSpinnerStyle" parent="Widget.AppCompat.Spinner">
    <item name="android:layout_marginLeft">8dp</item>
    <item name="android:layout_marginRight">12dp</item>
  </style>

  <style name="RecurrenceOptionCreatorFreqSpinnerStyle"
      parent="BaseRecurrenceOptionCreatorFreqSpinnerStyle">
    <item name="android:background">@null</item>
    <item name="android:layout_marginTop">4dp</item>
    <item name="android:layout_marginBottom">4dp</item>
    <item name="android:layout_gravity">center_horizontal</item>
  </style>

  <style name="BaseRecurrenceOptionCreatorEndSpinnerStyle" parent="Widget.AppCompat.Spinner">
